@model App.Areas.SalaryManagement.Models.OvertimeSalary






<div class="mx-5">
    <div class="col-md-6 bg-white rounded shadow-lg">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="OvertimeSalaryId" />
            <div class="form-group mx-5">
                <label asp-for="OvertimeSalaryName" class="control-label"></label>
                <input asp-for="OvertimeSalaryName" class="form-control" />
                <span asp-validation-for="OvertimeSalaryName" class="text-danger"></span>
            </div>
            <div class="form-group mx-5">
                <label asp-for="moneyPerSession" class="control-label"></label>
                <input asp-for="moneyPerSession" class="form-control" />
                <span asp-validation-for="moneyPerSession" class="text-danger"></span>
            </div>
            <div class="form-group mx-5">
                <label asp-for="StartTime" class="control-label"></label>
                <input asp-for="StartTime" class="form-control" />
                <span asp-validation-for="StartTime" class="text-danger"></span>
            </div>
            <div class="form-group mx-5">
                <label asp-for="EndTime" class="control-label"></label>
                <input asp-for="EndTime" class="form-control" />
                <span asp-validation-for="EndTime" class="text-danger"></span>
            </div>
            
        </form>
    </div>
</div>

<div class="mx-5 mb-4 content-information mt-5">
    <a asp-action="Index">
        <i class="far fa-arrow-alt-circle-left"></i>
        Back
    </a>
</div>

